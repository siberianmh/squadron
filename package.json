{
  "name": "@squadrondev/root",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "build": "yarn turbo run build",
    "lint": "prettier --check \"./**/*.{ts,tsx,js,yaml,yml}\"",
