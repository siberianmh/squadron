    "rollup": "^3.2.3",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^4.10.1",
    "@changesets/cli": "^2.24.1",
    "turbo": "^1.3.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@types/node-fetch": "^2",
    "@types/node": "^18.11.18"
  }
}
